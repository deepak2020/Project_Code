package Java_Day_1;

public class static_keyword_3 {
	
	String bedroom = "";
	
	static String kitchen;

	public static void main(String[] args) {
		
		static_keyword_3 obj = new static_keyword_3();
		obj.changeValue();
		obj.displayValue();
		
		System.out.println("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@");

		static_keyword_3 obj1 = new static_keyword_3();
		obj1.displayValue();
	}
	
	public void changeValue(){
		bedroom="one bed added by changeValue";
		kitchen = "2 glass added by changeValue";
	}
	
	public void displayValue(){
		System.out.println(a);
		System.out.println(b);
	}
	
	
	

}
