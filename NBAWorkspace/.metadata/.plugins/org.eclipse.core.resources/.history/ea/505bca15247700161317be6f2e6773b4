package testPackage;

public class AccessModifier {

	public static void main(String[] args) {
		//Public
		//Private//
		//Default
		//protected
		
//		AccessModifier obj = new AccessModifier();
//		obj.sum();
		staticSum();

	}
	
	protected void sum (){
		System.out.println("I am private method");
	}
	
	static void staticSum(){
		System.out.println("I am static sum");
	}

}
