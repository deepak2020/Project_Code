package daytwo;

public class Exception_Handling {

	public static void main(String[] args) {
		
		int i = 10;
		
		try{
		
		int divide = i/0;
		
		catch (ArithmeticException e){
			System.out.println(e.getMessage());
			}
		

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
