package Java_Day_1;

public class Static_Keyword {
	
	String name;
	int rollNo;
	String school;

	public static void main(String[] args) {
		
		Static_Keyword student1 = Static_Keyword();
	

	}
	
	public void studentDetails(String studentName, int studentRollNo, String studentSchool){
		
		name = studentName;
		rollNo = studentRollNo;
		school = studentSchool;
	}
	
	
	
}
