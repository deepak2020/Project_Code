public class Iterattion {
	
	static int a;
	public static void main(String arg[]){
		System.out.println(a);
		Iterattion obj1 = new Iterattion();
		obj1.print();
		staticPrint();
	}
	
	public void print(){
		System.out.println(a);
	}

	public static void staticPrint(){
		a=10;
		System.out.println(a);
	}
}
