package day_1;

public class First_Program {

	public static void main(String[] args) {
		
		System.out.println("Hello! World");
		
		//Data Types
		boolean a = false;
		System.out.println(a);
		byte b = 127;
		System.out.println(b);
		short c = 32767;
		System.out.println(c);
		int d = 12345678;
	}

}
