package dayOne;

import java.util.Scanner;

public class If_Else {

	public static void main(String[] args) {
//		Scanner scan = new Scanner(System.in);
//		int a = scan.nextInt();
		
		for (int a = 4; a<=100; a++){
		
		if (a%2==0){
			System.out.println("Number is non-prime" + a);
		} else if (a%3==0){
			System.out.println("Number is non-prime " + a);
		}  else 
			System.out.println("Number is prime " + a);

		
	}
	}
}
