package daytwo;

public class Exception_Handling {

	public static void main(String[] args) {

		int i = 10;

		try {

			int divide = i / 2;
		} catch (ArithmeticException e) {
			System.out.println(e.getMessage());
		}catch (Exception e) {
			System.out.println(e.getMessage());
		} finally{
			System.out.println("I will always be executed");
		}
		
		System.out.println("I am deepak");

	}

}
