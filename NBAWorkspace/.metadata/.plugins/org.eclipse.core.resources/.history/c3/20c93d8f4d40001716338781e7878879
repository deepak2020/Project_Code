		for (int a = 2; a<=100; a++){
			
			boolean isPrime = true;
			
			for (int i =2; i<=a; i++){
				if (a%i==0){
					isPrime = false;
					break;
				}
			}
			
			
			
		
		if (a%2==0){
//			System.out.println("Number is non-prime" + a);
		} else if (a%3==0){
//			System.out.println("Number is non-prime " + a);
		}  else 
			System.out.println("Number is prime " + a);

		
	}
	}
}
