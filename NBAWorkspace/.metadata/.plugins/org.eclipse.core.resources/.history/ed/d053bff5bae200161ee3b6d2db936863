//		driver.findElement(By.xpath("//*[@id='submit']")).click();
		ArrayList<WebElement> link = new ArrayList<WebElement>();
		
		link =(ArrayList<WebElement>) driver.findElements(By.tagName("a"));
		driver.close();
		
//	XPATH
	}

}
