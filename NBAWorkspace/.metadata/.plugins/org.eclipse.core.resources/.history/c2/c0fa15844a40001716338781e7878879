package dayOne;

public class If_Else {

	public static void main(String[] args) {
		int a = 100;

		if (a < 10) {
			System.out.println("Number is less than 10");
		} else if (a > 10) {
			System.out.println("Number si greater than 10");
		} else 
			System.out.println("Number is equal to 10");
	}

}
